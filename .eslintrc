{
  "parser": "@babel/eslint-parser",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["standard"],
  "globals": {
    "document": "writable"
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
